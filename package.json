{
  "name": "global",
  "version": "1.0.0",
  "description": "first project",
  "main": "index.js",
  "scripts": {
    "start": "gulp"
  },
  "config": {
    "src": {
      "sass": "src/sass/main.sass",
      "html": "src/",
      "js": "src/js/main.js",
      "fonts": "src/fonts/**/*.*",
      "img": {
        "compress": [
          "src/img/**/*.*",
          "!src/img/nocompress/**/*.*"
        ],
        "nocompress": "src/img/nocompress/**/*.*"
      }
    },
    "dist": {
      "css": "dist/css/",
      "html": "dist/",
      "js": "dist/js/",
      "fonts": "dist/fonts/",
      "img": "dist/img/"
    },
    "libs": {
      "js" : "src/js/libs.js",
      "css" : "src/css/libs.css"
    }
  },
  "author": "andry.urechko",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.24.7",
    "cssnano": "^4.1.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-buble": "^0.9.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-file-include": "^2.0.1",
    "gulp-imagemin": "^4.1.0",
    "gulp-include": "^2.3.1",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.1",
    "node-sass-tilde-importer": "^1.0.2",
    "require-dir": "^1.0.0",
    "run-sequence": "^2.2.1"
  },
  "dependencies": {}
}
